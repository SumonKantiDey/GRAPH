///Bidirectional graph///
#include <bits//stdc++.h>

using namespace std;
#define mx 20010
int parent[mx];
vector<int> edge[mx], cost[mx];
const int infinity = 1000000000;

struct data {
    int city, dist;
    bool operator < ( const data& p ) const {
        return dist > p.dist;
    }
};
void path(int parent[],int j)
 {
    // cout<<"j====="<<j<<"    "<<parent[j]<<endl;
     if(parent[j] == -1)return ;
     path(parent,parent[j]);
     cout<<j<<" ";
 }
int dijkstra(int source, int destination) {
    int d[mx];
    for(int i=0; i<=mx; i++) d[i] = infinity;

    priority_queue<data> q;
    memset(parent, -1, sizeof (parent));
    data u, v;
    u.city = source, u.dist = 0;
    q.push(u);
    d[source] = 0;
    parent[source] = 0;

    while(!q.empty()) {
        u = q.top(); q.pop();
        int ucost = d[u.city];

        for(int i=0; i<edge[u.city].size(); i++) {
            v.city = edge[u.city][i], v.dist = cost[u.city][i] + ucost;
            // relaxing :)
            if(d[v.city] > v.dist) {
                d[v.city] = v.dist;
                parent[v.city] = u.city;
                q.push( v );
            }
        }
    }
    return d[destination];
}

int main()
{
    int n, e, src, dist;
    while(scanf("%d %d", &n, &e)==2) {
        while(e--) {
                int u, v, w;
            cin>>u>>v>>w;
            edge[u].push_back(v);
            edge[v].push_back(u);
            cost[u].push_back(w);
            cost[v].push_back(w);
        }
        cin>>src>>dist;
        int ans = dijkstra(src, dist);
        cout<<"mimimun distance is: "<<ans<<endl;
        path(parent,dist);
        cout<<endl;
    }
    return 0;
}
/**
input output:
5 6
1 2 5
1 4 15
1 5 12
2 3 2
3 4 3
4 5 2
1 5 ///source to distance
minimum distance is: 12
1 5 ///path is

5 6
1 2 5
1 4 15
